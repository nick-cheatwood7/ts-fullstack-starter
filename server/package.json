{
  "name": "ts-fullstack-server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "nodemon dist/src/index.js",
    "build": "tsc",
